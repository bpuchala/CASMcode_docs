        ]
    },
    {
        "id": "sec3.1",
        "desc": "{% include tutorials/init/sec3/sec3.1-desc.md %}",
        "cmd": [
          "ls -h -l -a ."
        ]
    },
    {
        "id": "sec3.2",
        "desc": "{% include tutorials/init/sec3/sec3.2-desc.md %}",
        "cmd": [
          "casm sym -h",
          "casm sym",
          "casm sym --factor-group",
          "casm sym --lattice-point-group",
          "casm sym --crystal-point-group"
        ]
    },
    {
        "id": "sec3.3",
